    public string GetIncotermsTableHTML(string lang)
    {
        // Definimos las columnas: Code | Applicable To | Description
        var data = new List<string[]>();
        string col1Header, col2Header, col3Header;

        if (lang == "EN")
        {
            col1Header = "Incoterm"; col2Header = "Applicable to"; col3Header = "Description";

            data.Add(new[] { "EXW (Ex Works)", "For any mode of transport", "Seller makes goods available at their premises; buyer bears all subsequent costs and risks." });
            data.Add(new[] { "FCA (Free Carrier)", "For any mode of transport", "Seller delivers goods to the carrier chosen by the buyer at an agreed location." });
            data.Add(new[] { "CPT (Carriage Paid To)", "For any mode of transport", "Seller pays transport to the destination; risk transfers when goods are handed to the carrier." });
            data.Add(new[] { "CIP (Carriage and Insurance Paid To)", "For any mode of transport", "Same as CPT, but seller must provide minimum cargo insurance." });
            data.Add(new[] { "DAP (Delivered at Place)", "For any mode of transport", "Seller delivers goods ready for unloading at destination; buyer handles import clearance." });
            data.Add(new[] { "DPU (Delivered at Place Unloaded)", "For any mode of transport", "Same as DAP, but seller also unloads goods at destination." });
            data.Add(new[] { "DDP (Delivered Duty Paid)", "For any mode of transport", "Seller bears all costs, including import duties and customs clearance." });
            data.Add(new[] { "FAS (Free Alongside Ship)", "For sea or inland waterway transport", "Seller places goods alongside the vessel at the port of shipment." });
            data.Add(new[] { "FOB (Free On Board)", "For sea or inland waterway transport", "Seller delivers goods once they are on board the vessel." });
            data.Add(new[] { "CFR (Cost and Freight)", "For sea or inland waterway transport", "Seller pays freight to destination port; risk transfers when goods are on board." });
            data.Add(new[] { "CIF (Cost, Insurance & Freight)", "For sea or inland waterway transport", "Same as CFR, but seller must provide minimum insurance." });
            data.Add(new[] { "DEL (Delivery to...)", "Generic term", "No official ICC Incoterms rules" });
        }
        else // Español
        {
            col1Header = "Incoterm"; col2Header = "Aplicable a"; col3Header = "Descripción";

            data.Add(new[] { "EXW (Ex Works)", "Cualquier modo", "Vendedor pone mercancía a disposición en su local; comprador asume todo lo demás." });
            data.Add(new[] { "FCA (Free Carrier)", "Cualquier modo", "Vendedor entrega al transportista designado por el comprador en un lugar convenido." });
            data.Add(new[] { "CPT (Carriage Paid To)", "Cualquier modo", "Vendedor paga transporte hasta destino; riesgo se transfiere al comprador al entregar al transportista." });
            data.Add(new[] { "CIP (Carriage and Insurance Paid To)", "Cualquier modo", "Igual que CPT, pero vendedor contrata seguro mínimo; mejor protección." });
            data.Add(new[] { "DAP (Delivered at Place)", "Cualquier modo", "Vendedor entrega la mercancía en un lugar convenido en destino; comprador asume importación/aranceles." });
            data.Add(new[] { "DPU (Delivered at Place Unloaded)", "Cualquier modo", "Igual que DAP, pero el vendedor además descarga la mercancía en destino. Esta regla sustituyó a la antigua “DAT”." });
            data.Add(new[] { "DDP (Delivered Duty Paid)", "Cualquier modo", "Vendedor asume todos los costos, transporte, descarga, aranceles — entrega en destino listo para comprador." });
            data.Add(new[] { "FAS (Free Alongside Ship)", "Solo marítimo/vías fluviales", "Vendedor entrega al costado del barco en puerto embarque; comprador asume desde ahí." });
            data.Add(new[] { "FOB (Free On Board)", "Solo marítimo/vías fluviales", "Vendedor entrega cuando mercancía supera la borda del buque; luego riesgo/pago lo asume comprador." });
            data.Add(new[] { "CFR (Cost and Freight)", "Solo marítimo/vías fluviales", "Vendedor paga transporte hasta puerto destino; riesgo se transfiere cuando la mercancía cruza la borda." });
            data.Add(new[] { "CIF (Cost, Insurance & Freight)", "Solo marítimo/vías fluviales", "Igual que CFR, pero el vendedor contrata seguro (nivel mínimo por defecto)." });
            data.Add(new[] { "DEL (Delivery to...)", "Término genérico", "No es oficial en las reglas de ICC Incoterms" });
        }

        var sb = new System.Text.StringBuilder();
        // Estilo de tabla con hover y bordes azules
        sb.Append("<div class='table-responsive'>");
        sb.Append("<table class='table table-sm table-bordered table-hover' style='border: 1px solid #009ff5; font-size: 0.85rem;'>");

        // Encabezado Azul
        sb.Append("<thead style='background-color: #009ff5; color: white; text-align:center;'>");
        sb.Append("<tr>");
        sb.Append($"<th style='width: 15%; border-right: 1px solid white;'>{col1Header}</th>");
        sb.Append($"<th style='width: 25%; border-right: 1px solid white;'>{col2Header}</th>");
        sb.Append($"<th style='width: 60%;'>{col3Header}</th>");
        sb.Append("</tr></thead>");

        sb.Append("<tbody>");
        foreach (var row in data)
        {
            sb.Append("<tr>");
            // Columna 1: Negrita, fondo azul muy claro
            sb.Append($"<td style='background-color: #e6f7ff; font-weight:bold;'>{row[0]}</td>");
            // Columna 2: Normal
            sb.Append($"<td style='vertical-align:middle;'>{row[1]}</td>");
            // Columna 3: Normal
            sb.Append($"<td style='vertical-align:middle;'>{row[2]}</td>");
            sb.Append("</tr>");
        }
        sb.Append("</tbody></table></div>");

        return sb.ToString();
    }

    public string GetIncotermsResponsibilityChartHTML()
    {
        var sb = new System.Text.StringBuilder();

        // Colores basados en la imagen
        string colorBlue = "#a9cce3"; // Color para Buyer
        string colorGray = "#f2f3f4"; // Color para Seller
        string colorHeader = "#154360"; // Azul oscuro headers
        string colorRisk = "#e74c3c"; // Rojo Risk
        string colorTxtWhite = "#ffffff";

        // Estilos base para celdas
        string styleCell = "border: 1px solid #ccc; padding: 4px; text-align: center; font-size: 11px;";
        string styleHeader = $"background-color: {colorHeader}; color: {colorTxtWhite}; border: 1px solid #fff; font-weight: bold;";
        string styleRisk = $"background-color: {colorRisk}; color: {colorTxtWhite}; font-weight: bold; font-size: 10px;";

        sb.Append("<table style='width: 100%; border-collapse: collapse; font-family: Arial, sans-serif;'>");

        // --- ENCABEZADOS ---
        // Fila 1: Grupos
        sb.Append("<thead>");
        sb.Append("<tr>");
        sb.Append($"<th style='{styleCell} background-color: #d4edda; width: 15%;'>Groups</th>"); // Verde claro
        sb.Append($"<th colspan='2' style='{styleCell} background-color: #d4edda;'>Any Mode of Transport</th>");
        sb.Append($"<th colspan='4' style='{styleCell} background-color: #d4edda;'>Sea / Inland Waterway</th>");
        sb.Append($"<th colspan='5' style='{styleCell} background-color: #d4edda;'>Any Mode of Transport</th>");
        sb.Append("</tr>");

        // Fila 2: Siglas (Incoterm)
        sb.Append("<tr>");
        sb.Append($"<th style='{styleCell} {styleHeader} font-size: 14px;'>Incoterm</th>");
        string[] terms = { "EXW", "FCA", "FAS", "FOB", "CFR", "CIF", "CPT", "CIP", "DAP", "DPU", "DDP" };
        foreach (var t in terms) { sb.Append($"<th style='{styleCell} {styleHeader} font-size: 13px;'>{t}</th>"); }
        sb.Append("</tr>");

        // Fila 3: Transfer of Risk (Roja)
        sb.Append("<tr>");
        sb.Append($"<th style='{styleCell} {styleRisk}'>Transfer of Risk</th>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>At Buyer's Disposal</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>On Buyer's Transport</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>Alongside Ship</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>On Board</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>On Board</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>On Board</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>At Carrier</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>At Carrier</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>At Named Place</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>At Place Unloaded</td>");
        sb.Append($"<td style='{styleCell} {styleRisk}'>At Named Place</td>");
        sb.Append("</tr>");
        sb.Append("</thead>");

        // --- CUERPO (OBLIGACIONES) ---
        sb.Append("<tbody>");

        // Definimos los datos. "S" = Seller (Gris), "B" = Buyer (Azul), "N" = Negotiable (Blanco)
        var rows = new List<Tuple<string, string[]>>
        {
            new Tuple<string, string[]>("Export Packaging", new[] { "S", "S", "S", "S", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Loading Charges", new[] { "B", "S", "S", "S", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Delivery to Port/Place", new[] { "B", "S", "S", "S", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Export Duty/Taxes", new[] { "B", "S", "S", "S", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Origin Terminal Charges", new[] { "B", "B", "S", "S", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Loading on Carriage", new[] { "B", "B", "B", "S", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Carriage Charges", new[] { "B", "B", "B", "B", "S", "S", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Insurance", new[] { "N", "N", "N", "N", "N", "S*", "N", "S**", "N", "N", "N" }), // Casos especiales
            new Tuple<string, string[]>("Dest. Terminal Charges", new[] { "B", "B", "B", "B", "B", "B", "S", "S", "S", "S", "S" }),
            new Tuple<string, string[]>("Delivery to Destination", new[] { "B", "B", "B", "B", "B", "B", "B", "B", "S", "S", "S" }),
            new Tuple<string, string[]>("Unloading at Dest.", new[] { "B", "B", "B", "B", "B", "B", "B", "B", "B", "S", "S" }), // DPU Seller unloads
            new Tuple<string, string[]>("Import Duty/Taxes", new[] { "B", "B", "B", "B", "B", "B", "B", "B", "B", "B", "S" })
        };

        foreach (var row in rows)
        {
            sb.Append("<tr>");
            // Celda del título de la fila (Header lateral)
            sb.Append($"<td style='{styleCell} {styleHeader} text-align:left; padding-left:10px;'>{row.Item1}</td>");

            foreach (var val in row.Item2)
            {
                string bg = "#ffffff";
                string text = "";
                string fontWeight = "normal";

                if (val.StartsWith("S")) { bg = colorGray; text = "Seller"; }
                else if (val.StartsWith("B")) { bg = colorBlue; text = "Buyer"; }
                else if (val.StartsWith("N")) { bg = "#ffffff"; text = "Negotiable"; }

                // Manejo de asteriscos para seguro
                if (val.Contains("*")) text += val.Substring(1); // Agrega asteriscos si existen

                sb.Append($"<td style='{styleCell} background-color: {bg}; font-weight: {fontWeight};'>{text}</td>");
            }
            sb.Append("</tr>");
        }

        sb.Append("</tbody>");
        sb.Append("</table>");

        // Nota al pie
        sb.Append("<div style='margin-top:5px; font-size:10px; color:#666;'>");
        sb.Append("<strong>* CIF:</strong> Requires minimum insurance coverage (Clause C). <br>");
        sb.Append("<strong>** CIP:</strong> Requires 'All Risk' insurance coverage (Clause A).");
        sb.Append("</div>");

        return sb.ToString();
    }
}

