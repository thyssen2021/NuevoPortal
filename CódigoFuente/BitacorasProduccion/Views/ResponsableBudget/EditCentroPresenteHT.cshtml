    //render para target
    function conditionalRenderer(instance, td, row, col, prop, value, cellProperties) {
        // Llama al renderer por defecto para pintar el valor
        Handsontable.renderers.TextRenderer.apply(this, arguments);

        // Obtén el índice de la columna Total (Local - USD) y Target
        var totalCol = getColumnIndexByName("Total (Local - USD)");
        var targetCol = getColumnIndexByName("Target");

        

        // Obtén los valores de Total y Target para esta fila
        var totalValue = parseFloat(instance.getDataAtCell(row, totalCol)) || 0;
        var targetValue = parseFloat(instance.getDataAtCell(row, targetCol)) || 0;

        // Limpia estilos anteriores (si los hubiera)
        td.style.backgroundColor = "";

        // Aplica el estilo según la comparación
        if (targetValue > 0) { // evitar división por cero o comparación errónea
            if (totalValue > targetValue) {
                td.style.backgroundColor = "#FF9191"; // Total mayor que Target
            } else if (totalValue >= 0.8 * targetValue) {
                td.style.backgroundColor = "#FFCD90"; // Total mayor o igual al 80% de Target
            }
        }
    }


