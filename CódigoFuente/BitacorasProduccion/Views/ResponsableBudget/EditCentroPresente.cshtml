@section scripts
{
    <!-- SweetAlert2 -->
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render(Url.Content("~/Content/vendors/sweetalert2/sweetalert2.min.js"))
    @Scripts.Render(Url.Content("~/Scripts/AlertasSweetAlert.js"))
    @Scripts.Render(Url.Content("~/Scripts/MvcFoolproofJQueryValidation.js"))
    @Scripts.Render("~/bundles/dataTables_js")

    <script>
        $(document).ready(function () {

            jQuery.extend(jQuery.validator.messages, {
                step: jQuery.validator.format("Dos decimales!")
            });

            //inicializa los data table
            var tabla = $('#datatable_1').DataTable({
                "paging": false,
                "ordering": false,
                "searching": false,
                "scrollX": true,
                "scrollY": "400px",
                "info": false,
                fixedColumns: {
                    left: 4
                }
            });

            var tabla2 = $('#datatable_2').DataTable({
                "paging": false,
                "ordering": false,
                "searching": false,
                "scrollX": true,
                "scrollY": "400px",
                "info": false,
                fixedColumns: {
                    left: 4
                },
            });

            var tabla3 = $('#datatable_3').DataTable({
                "paging": false,
                "ordering": false,
                "searching": false,
                "scrollX": true,
                "scrollY": "400px",
                "info": false,
                fixedColumns: {
                    left: 4
                }
            });

            //Agrega el evento a todos los input
            $(":input").each(function () {
                $(this).on('input', function (e) {

                    calculaTotal();
                    calculaFila($(this).data("fila"));
                    calculaColumna($(this).data("columna"));

                });
            });

          

        });

        

        function calculaFila(fila) {
            let sumaFila = 0;
            $("*[data-fila='" + fila + "']").each(function () {
                sumaFila = sumaFila + ConvierteAFloat($(this).val(), 0)
            });

            sumaFila = sumaFila.toFixed(2);

            let total_actual = ConvierteAFloat($("#total_inicial_fila_" + fila).val(), 0);
            total_actual = total_actual.toFixed(2);

            let total = parseFloat(total_actual) + parseFloat(sumaFila);

            $("#total_fila_" + fila).html(total.toLocaleString('en-US', { style: 'currency', currency: 'USD', minimumFractionDigits: 2 }));

        }

        function calculaColumna(columna) {
            let sumaColumna = 0;
            $("*[data-columna='" + columna + "']").each(function () {
                sumaColumna = sumaColumna + ConvierteAFloat($(this).val(), 0)
            });

            $("#total_columna_" + columna).html(sumaColumna.toLocaleString('en-US', { style: 'currency', currency: 'USD', minimumFractionDigits: 2 }));

        }

        function calculaTotal() {
            var suma = 0;

            $('.cantidad').each(function () {
                suma = suma + ConvierteAFloat($(this).val(), 0)
            });

            suma = suma.toFixed(2);

            let total_actual = ConvierteAFloat($("#total_actual").val(), 0);
            total_actual = total_actual.toFixed(2);

            let total = parseFloat(total_actual) + parseFloat(suma);

            $("#total_fila_columna").html(total.toLocaleString('en-US', { style: 'currency', currency: 'USD', minimumFractionDigits: 2 }));

        }

        function ReiniciaTablaTimer() {

            setTimeout(ReiniciaTablas, 150);
            setTimeout(ReiniciaTablas, 220);
            setTimeout(ReiniciaTablas, 300);
        }

        function ReiniciaTablas() {
            clicMenu(2);
        }


        //agranda el tamaño de la barra
        window.onload = function () {
            clicMenu(1);
        }

        //muestra modal de confirmación de cerrar pagina

        // Warning
        $(window).on('beforeunload', function () {
            return "Any changes will be lost";
        });

        // Form Submit
        $(document).on("submit", "form", function (event) {
            // disable unload warning
            $(window).off('beforeunload');
        });
    </script>

