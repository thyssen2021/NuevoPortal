                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="info-card">
                                    <i class="fas fa-arrow-down info-icon"></i>
                                    <span class="titulo-card">Mínimo</span>
                                    <h3>@Model.Minimo.ToString("F3")</h3>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="info-card">
                                    <i class="fas fa-arrow-up info-icon"></i>
                                    <span class="titulo-card">Máximo</span>
