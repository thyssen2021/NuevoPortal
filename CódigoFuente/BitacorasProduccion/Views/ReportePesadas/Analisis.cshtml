                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="info-card">
                                    <i class="fas fa-chart-line info-icon"></i>
                                    <span class="titulo-card">Desviación Estándar</span>
                                    <h3>@Model.DesviacionEstandar.ToString("F3")</h3>
