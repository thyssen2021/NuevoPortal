@model IEnumerable<Portal_2_0.Models.BG_IHS_item>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    //ViewBag.Title = "Listado de IHS";
    ViewBag.PrimerNivel = "budget_ihs";
    ViewBag.SegundoNivel = "listado_ihs";
    ViewBag.ControllerName = "BG_IHS_item";

    Clases.Util.MensajesSweetAlert mensajeAlerta = (Clases.Util.MensajesSweetAlert)ViewBag.MensajeAlert;

    //obtiene los filtros
    Portal_2_0.Models.Paginacion paginacionFiltros = (Portal_2_0.Models.Paginacion)ViewBag.Paginacion;


    string manufacturer = string.Empty, production_plant = String.Empty, vehicle = String.Empty, origen = String.Empty, country_territory = String.Empty;
    string demanda = String.Empty;

    if (paginacionFiltros.ValoresQueryString["manufacturer"] != null)
    {
        manufacturer = paginacionFiltros.ValoresQueryString["manufacturer"].ToString();
    }
    if (paginacionFiltros.ValoresQueryString["production_plant"] != null)
    {
        production_plant = paginacionFiltros.ValoresQueryString["production_plant"].ToString();
    }
    if (paginacionFiltros.ValoresQueryString["vehicle"] != null)
    {
        vehicle = paginacionFiltros.ValoresQueryString["vehicle"].ToString();
    }
    if (paginacionFiltros.ValoresQueryString["origen"] != null)
    {
        origen = paginacionFiltros.ValoresQueryString["origen"].ToString();
    }
    if (paginacionFiltros.ValoresQueryString["country_territory"] != null)
    {
        country_territory = paginacionFiltros.ValoresQueryString["country_territory"].ToString();
    }
    if (paginacionFiltros.ValoresQueryString["demanda"] != null)
    {
        demanda = paginacionFiltros.ValoresQueryString["demanda"].ToString();
    }

    var cabeceraDemanda = Portal_2_0.Models.BG_IHS_UTIL.GetCabecera();
    var cabeceraCuartos = Portal_2_0.Models.BG_IHS_UTIL.GetCabeceraCuartos();
    var cabeceraAnios = Portal_2_0.Models.BG_IHS_UTIL.GetCabeceraAnios();
}

@section estilos
{
    <!-- DataTables -->
    @Styles.Render("~/Content/dataTables_css")
    <!--fixed columns css-->
    <link href="@Url.Content("~/Content/vendors/datatables.net-fixedcolumns/dataTables.fixedColumns.css")" rel="stylesheet">

    <!-- SweetAlert2 -->
    <link href="@Url.Content("~/Content/vendors/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css")" rel="stylesheet">

    <!-- Select2 -->
    <link href="@Url.Content("~/Content/vendors/select2/css/select2.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/vendors/select2-bootstrap4-theme/select2-bootstrap4.min.css")" rel="stylesheet">

    <style>
        .fondo-default {
            background-color: #f7f7f7 !important;
            color: #000000 !important;
        }

        .fondo-ihs {
            background-color: antiquewhite !important;
        }

        .fondo-user {
            background-color: #b0fbf6 !important;
        }

        .fondo-original {
            background-color: #faff6b !important;
        }

        .fondo-customer {
            background-color: #6afcf3 !important;
        }

        .fondo-cuarto-calculado {
            background-color: #bbf3c1 !important;
            color: darkslategray !important;
        }

        .fondo-cuarto-ihs {
            background-color: lightpink !important;
            color: darkslategray !important;
        }
    </style>
}

