@section scripts
{

    @Scripts.Render(Url.Content("~/Content/vendors/select2/js/select2.full.min.js"))
    <!-- SweetAlert2 -->
    @Scripts.Render(Url.Content("~/Content/vendors/sweetalert2/sweetalert2.min.js"))
    @Scripts.Render(Url.Content("~/Scripts/AlertasSweetAlert.js"))

    @Scripts.Render("~/bundles/dataTables_js")
    <!-- iCheck -->
    @Scripts.Render(Url.Content("~/Content/vendors/iCheck/icheck.min.js"))

    <script>

        $(document).ready(function () {
            //agranda el tamaño de la barra
            window.onload = function () {
                document.getElementById('menu_toggle').click();
            }

            //inicializa los data table
            $('#datatable_1').DataTable({
                "paging": false,
                "ordering": false,
                "searching": false,
                "scrollX": true,
                "info": false
            });



            $('#tipo_reporte_sabana').on('ifChanged', function (event) {
                actualizaFormulario();
            });

            seleccionaValoresDefault();


            // Initialize Select2 Elements
            $('.select2bs4').select2({
                theme: 'bootstrap4'
            })

            $("#clave_planta").change(function () {

                var cPlanta = document.getElementById("clave_planta");

                //realiza llamada para lineas
                $.ajax({
                    type: 'POST',
                    url: '/Combos/obtieneLineasPlantas',
                    data: { clavePlanta: cPlanta.value },
                    success: function (data) {
                        populateDropdown($("#id_linea"), data);
                    },
                    async: false
                });

            });

        });

        //muestra/oculta campos de tipo de reporte
        function actualizaFormulario() {
            if ($("#tipo_reporte_sabana").is(':checked')) {
                $("#div_sabana").fadeIn();
                $("#div_turno").hide();
                $('#turno').prop("required", false);
                $('#fecha_turno').prop("required", false);
            } else {
                $("#div_sabana").hide();
                $("#div_turno").fadeIn();
                $('#turno').prop("required", true);
                $('#fecha_turno').prop("required", true);
            }
        }

        //activa y desactiva los combos
        function verificaEstadoCombos() {

            var planta = $("#clave_planta option:selected").val();

            //si planta esta vacia
            if (planta == "") {
                $("#id_linea").prop("disabled", true);
                //  $("#crearFormButton").prop("disabled", true);
            }
            else {
                $("#id_linea").prop("disabled", false);
            }


        }


        //selecciona los valores que carga por defecto
        function seleccionaValoresDefault() {

            var cPlanta = document.getElementById("clave_planta");
            var cLinea = document.getElementById("cLinea");

            //Si hay valores previos (EDIT)
            if (cPlanta != null && cPlanta.value != 0) {
                //estable el valor del primer combo
                if ($("#clave_planta option[value='" + cPlanta.value + "']").length > 0) {
                    $("#clave_planta").val(cPlanta.value);
                } else {
                    $("#clave_planta").val("");
                }

                //realiza llamada para lineas
                $.ajax({
                    type: 'POST',
                    url: '/Combos/obtieneLineasPlantas',
                    data: { clavePlanta: cPlanta.value },
                    success: function (data) {
                        populateDropdown($("#id_linea"), data);
                    },
                    async: false
                });

                if (cLinea != null && cLinea.value != 0) {
                    //establece el valor del segundo select una vez termina la llamada ajax
                    if ($("#id_linea option[value='" + cLinea.value + "']").length > 0) {
                        $("#id_linea").val(cLinea.value);
                    } else {
                        $("#id_linea").val("");
                    }
                }

            } else { //en caso de que no haya valores previos (CREATE)
                verificaEstadoCombos();
            }

            //verifica el tipo de reporte
            let tipo_reporte = document.getElementById("cTipoReporte");
            if (tipo_reporte != null && tipo_reporte.value == "turno") {
                $('#tipo_reporte_turno').prop("checked", true);
            } else {
                $('#tipo_reporte_sabana').prop("checked", true);
            }

            actualizaFormulario();



            // Initialize Select2 Elements
            $('.select2bs4').select2({
                theme: 'bootstrap4'
            })

        }

        //completa el selec con los datos recibidos
        function populateDropdown(select, data) {
            select.html('');
            $.each(data, function (id, option) {
                select.append($('<option></option>').val(option.value).html(option.name));
            });
        }

        //borra los datos del formulario para restablecerlo
        var form = document.getElementById("buscarForm");


    </script>

}

